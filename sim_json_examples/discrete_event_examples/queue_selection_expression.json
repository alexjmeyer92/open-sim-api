{
  "processes": [
    {
      "name": "Pick",
      "duration": 1,
      "output_queue_selection": {
        "type": "all"
      },
      "output_queues": [
        {
          "name": "ship_staging",
          "rate": {
            "type": "static",
            "static_value": 3
          }
        }
      ]
    },
    {
      "name": "Ship",
      "duration": 1,
      "input_queue_selection": {
        "type": "expression",
        "expression": "lambda **kwargs : 1 if kwargs['now'] >= 10 else 0"
      },
      "input_queues": [
        {
          "name": "ship_staging",
          "rate": {
            "type": "static",
            "static_value": 3
          }
        },
        {
          "name": "ship_ready",
          "rate": {
            "type": "static",
            "static_value": 3
          }
        }
      ],
      "output_queue_selection": {
        "type": "all"
      },
      "output_queues": [
        {
          "name": "shipped",
          "rate": {
            "type": "static",
            "static_value": 3
          }
        }
      ]
    }
  ],
  "queues": [
    {
      "name": "shipped",
      "capacity": 10000,
      "inital_value": 0,
      "queue_type": "continuous"
    },
    {
      "name": "ship_staging",
      "capacity": 200,
      "inital_value": 30,
      "queue_type": "continuous"
    },
    {
      "name": "ship_ready",
      "capacity": -1,
      "inital_value": 100,
      "queue_type": "continuous"
    }
  ],
  "sim_def": {
    "name": "Pick and Ship Simulation",
    "epochs": 100,
    "time_unit": "seconds"
  }
}